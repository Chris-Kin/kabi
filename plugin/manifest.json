{
    "manifest_version": 2,
    "name": "Fill Form",
    "version": "1.2",
    "description": "The first extension that I made.",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "http://*/",
        "bookmarks",
        "tabs",
        "history"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.baidu.com/*"
            ],
            "css": [
                "test.css"
            ],
            "js": [
                "test.js"
            ]
        }
    ]
}